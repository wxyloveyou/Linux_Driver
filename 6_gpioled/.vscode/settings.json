{
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.o":true,
        "**/*.su":true, 
        "**/*.cmd":true,
        "Documentation":true,      
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,  
        "**/*.o":true,
        "**/*.su":true, 
        "**/*.cmd":true,
        "Documentation":true, 
    },
    "files.associations": {
        "kernel.h": "c",
        "delay.h": "c",
        "module.h": "c",
        "init.h": "c",
        "compiler.h": "c",
        "types.h": "c",
        "fs.h": "c",
        "uaccess.h": "c"
